{
  "name": "express-test",
  "version": "0.0.0",
  "scripts": {
    "build": "./node_modules/.bin/ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@services": "dist/services",
    "@repos": "dist/repos",
    "@entities": "dist/entities",
    "@server": "dist/server",
    "@routes": "dist/routes",
    "@lib": "dist/lib"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "eslintIgnore": [
    "src/public/",
    "build.ts"
  ],
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@graphql-tools/schema": "^8.4.0",
    "@prisma/client": "^3.15.2",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-sqlite3": "^0.9.1",
    "@types/debug": "^4.1.7",
    "@types/express-session": "^1.17.4",
    "@types/ioredis": "^4.28.10",
    "@types/passport": "^1.0.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/ramda": "^0.28.14",
    "@types/sha256": "^0.2.0",
    "@types/ws": "^8.5.3",
    "apollo-server": "^3.9.0",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "class-validator": "^0.13.2",
    "command-line-args": "^5.2.1",
    "connect-sqlite3": "^0.9.13",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "graphql": "^15.8.0",
    "graphql-redis-subscriptions": "^2.4.2",
    "graphql-ws": "^5.9.0",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.0.6",
    "jet-logger": "^1.1.5",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node-django-hashers": "^1.1.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-keycloak-oauth2-oidc": "^1.0.5",
    "prisma-dbml-generator": "^0.9.1",
    "ramda": "^0.28.0",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "remeda": "^0.0.35",
    "sha256": "^0.2.0",
    "socket.io": "^4.5.1",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.20.4",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^3.10.6",
    "@types/jsonfile": "^6.1.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "find": "^0.3.0",
    "fs-extra": "^10.1.0",
    "jasmine": "^4.2.1",
    "nodemon": "^2.0.16",
    "prisma": "^3.15.2",
    "socket.io-client": "^4.5.1",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.4"
  }
}
